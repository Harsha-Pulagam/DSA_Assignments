   "source": [
    "#TC = O(n)\n",
    "#SC = O(n)\n",
    "def firstUniqChar(s):\n",
    "    d ={}\n",
    "    for x in s:\n",
    "        if x in d:\n",
    "            d[x] +=1\n",
    "        else:\n",
    "            d[x] = 1\n",
    "    for i in range(len(s)):\n",
    "        if d[s[i]]==1:\n",
    "            return i\n",
    "    return -1"
   ]
